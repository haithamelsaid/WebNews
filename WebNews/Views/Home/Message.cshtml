@model List<ContactUs>
    <div class="col-10 fa-align-center" style="margin-left: auto; margin-right: auto;" >
        <table class="table table-dark table-sm">
            <thead>
                <tr style="height:50px;padding-top:10px">
                    <th>Name</th>
                    <th>Email</th>
                    <th>Subject</th>
                    <th>Message</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr style="height:50px;padding-bottom:10px">
                        <td>@item.Name</td>
                        <td>@item.Email</td>
                        <td>@item.Subject</td>
                        <td>@item.Message</td>
                    </tr>
                }
            </tbody>
    </table>
</div>